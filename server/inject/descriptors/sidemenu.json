          },
          "link": {
            "type": "action",
            "selector": "li a",
            "control": "action.link",
            "selectContains": "href"
          }
        }
      },
      "update": {
        "type": "none"
      },
      "selectorAll": ".menubar ul li"
    },
    "ismenu": {
      "type": "identifier",
      "selector": ".menubar",
      "control": "exists"
    }
  },
  "connect": []
}
