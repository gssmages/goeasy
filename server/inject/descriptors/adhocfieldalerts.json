{
  "type": "object",
  "comment": "",
  "properties": {
    "errorInfo": {
      "type": "string",
      "selector": "#messageinput",
      "control": "property.innerText"
    },
    "okButton": {
      "type": "action",
      "selector": "#myBtn",
      "control": "action.button"
    },
    "isAdhocAlert": {
      "type": "identifier",
      "selector": {
        "fn": "function(node){if(document.querySelector('.usermenu li.selected').innerText=='Adhoc Request'){if(node.style.display=='block'){return true}}else{return false}}",
        "selector": ""
      },
      "control": "exists"
    },
    "back": {
      "type": "action",
      "selector": ".close",
      "control": "action.button"
    }
  },
  "connect": [
    {
      "match": [
        "isAdhocAlert"
      ],
      "screens": [
        "adhocfieldalerts"
      ],
      "model": "adhocfieldalerts"
    }
  ],
  "selector": "#myModal"
}
