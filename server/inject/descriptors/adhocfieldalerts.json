{
  "type": "object",
  "comment": "",
  "properties": {
    "errorInfo": {
      "type": "string",
      "selector": "#messageinput",
      "control": "property.innerText"
    },
    "okButton": {
      "type": "action",
      "selector": "#myBtn",
      "control": "action.button"
    },
    "isAdhocAlert": {
      "type": "identifier",
      "control": "exists",
      "frameSelector": [
        null,
        ""
      ],
      "selector": "#myModal"
    },
    "back": {
      "type": "action",
      "selector": ".close",
      "control": "action.button"
    }
  },
  "connect": [
    {
      "match": [
        "isAdhocAlert"
      ],
      "screens": [
        "adhocfieldalerts"
      ],
      "model": "adhocfieldalerts"
    }
  ]
}
