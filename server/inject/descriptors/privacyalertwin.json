{
  "type": "object",
  "comment": "",
  "properties": {
    "privacyInfo": {
      "type": "string",
      "selector": ".modal-body",
      "control": "property.innerText"
    },
    "No": {
      "type": "action",
      "selector": "input[type=button]",
      "control": "action.button",
      "selectIndex": 1
    },
    "Yes": {
      "type": "action",
      "selector": "#myBtnAccept",
      "control": "action.button"
    },
    "isPrivacyAlert": {
      "type": "identifier",
      "control": "exists",
      "selector": {
        "fn": "function(node){if(node.style.display=='block'&&(document.querySelector('.usermenu li.selected').innerText=='Adhoc Request')){return true}}",
        "selector": ""
      }
    }
  },
  "connect": [
    {
      "match": [
        "isPrivacyAlert"
      ],
      "screens": [
        "privacyalertwin"
      ],
      "model": "privacyalertwin"
    }
  ],
  "selector": "#myModalPrivacy"
}
