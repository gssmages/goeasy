{
  "type": "object",
  "comment": "SlideOutMenu",
  "properties": {
    "logout": {
      "type": "action",
      "control": "property.innerText"
    },
    "sidemenuitem": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "menuname": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "li a div",
            "control": "property.innerText"
          },
          "link": {
            "type": "action",
            "selector": "li a",
            "control": "action.link",
            "selectContains": "href"
          }
        }
      },
      "update": {
        "type": "none"
      },
      "selectorAll": ".menubar"
    },
    "ismenu": {
      "type": "identifier",
      "selector": "a",
      "control": "action.link"
    }
  },
  "connect": []
}
