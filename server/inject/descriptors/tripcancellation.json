{
  "type": "object",
  "comment": "",
  "properties": {
    "FromDate": {
      "type": "date",
      "format": "DD/MM/YYYY",
      "selector": "#regularfromdate",
      "control": "text"
    },
    "ToDate": {
      "type": "date",
      "format": "DD/MM/YYYY",
      "selector": "#regulartodate",
      "control": "text"
    },
    "RequestFor": {
      "type": "select",
      "selector": "#ddlRequestFor",
      "control": "select"
    },
    "saveButton": {
      "type": "action",
      "selector": "#myBtnSave",
      "control": "action.button",
      "selectContains": "Save"
    },
    "closebutton": {
      "type": "action",
      "selector": "#myBtnClose",
      "control": "action.button",
      "selectContains": "Close"
    },
    "ispmodel": {
      "type": "identifier",
      "selector": "#myRegularPopup",
      "control": "exists"
    }
  },
  "connect": [
    {
      "match": [
        "isCancelPermanentTrip",
        "isCPT",
        "ispmodel"
      ],
      "screens": [
        "tripcancellation"
      ],
      "model": "tripCancellation"
    }
  ]
}
