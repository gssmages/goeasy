{
  "type": "object",
  "comment": "",
  "properties": {
    "tripInfo": {
      "type": "string",
      "selector": ".modal-body",
      "control": "property.innerText"
    },
    "yes": {
      "type": "action",
      "selector": "#myBtnYes",
      "control": "action.button"
    },
    "no": {
      "type": "action",
      "selector": "#myBtnNo",
      "control": "action.button"
    },
    "isAdhocCancellation": {
      "type": "identifier",
      "control": "exists"
    },
    "back": {
      "type": "action",
      "selector": ".close",
      "control": "action.button"
    }
  },
  "connect": [
    {
      "match": [
        "isAdhocCancellation"
      ],
      "screens": [
        "adhoctripcancellation"
      ],
      "model": "adhoctripcancellation"
    }
  ],
  "selector": "#myAlertModal"
}
