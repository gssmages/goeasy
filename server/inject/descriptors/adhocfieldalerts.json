{
  "type": "object",
  "comment": "",
  "properties": {
    "errorInfo": {
      "type": "string",
      "selector": "#messageinput",
      "control": "property.innerText"
    },
    "okButton": {
      "type": "action",
      "selector": "#myBtn",
      "selectContains": "OK",
      "control": "action.button"
    },
    "isAdhocAlert": {
      "type": "identifier",
      "selector": {
        "fn": "function(node){console.log(node.style.display)}",
        "selector": "#myModal"
      },
      "control": "exists"
    }
  },
  "connect": [
    {
      "match": [
        "isAdhocAlert"
      ],
      "screens": [
        "adhocfieldalerts"
      ]
    }
  ]
}
