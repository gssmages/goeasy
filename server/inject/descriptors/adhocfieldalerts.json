{
  "type": "object",
  "comment": "",
  "properties": {
    "errorInfo": {
      "type": "string",
      "selector": ".modal-body",
      "control": "property.innerText"
    },
    "okButton": {
      "type": "action",
      "selector": "#myBtn",
      "control": "action.button"
    },
    "isAdhocAlert": {
      "type": "identifier",
      "control": "exists",
      "frameSelector": [
        null,
        ""
      ],
      "selector": {
        "fn": "function(node){if(node.style.display=='block'){return true}}",
        "selector": ""
      }
    },
    "back": {
      "type": "action",
      "selector": ".close",
      "control": "action.button"
    }
  },
  "connect": [
    {
      "match": [
        "isAdhocAlert"
      ],
      "screens": [
        "adhocfieldalerts"
      ],
      "model": "adhocfieldalerts"
    }
  ],
  "selector": "#myModal .modal-content"
}
